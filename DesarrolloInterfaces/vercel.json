{
  "redirects": [
    {
      "source": "/",
      "destination": "/NovelaGraficaApp/index.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/NovelaGraficaApp/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/NovelaGraficaApp/index.html"
    }
  ]
}